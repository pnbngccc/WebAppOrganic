@model Member
<!-- Contact Form Begin -->
<div class="contact-form spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="contact__form__title">
                    <h2>Register Member</h2>
                </div>
            </div>
        </div>
        <div asp-validation-summary="All"></div>
        <form method="post">
            <div class="row">
                <div class="col-lg-6 col-md-6">
                    <input type="text" asp-for="GivenName" placeholder="GiveName">
                </div>
                <div class="col-lg-6 col-md-6">
                    <input type="text" asp-for="Surname" placeholder="Surname">
                </div>
                <div class="col-lg-6 col-md-6">
                    <input type="email" asp-for="Email" placeholder="Email">
                </div>
                <div class="col-lg-6 col-md-6">
                    <input type="password" asp-for="Password" placeholder="Password">
                </div>
                <div class="col-lg-12 text-center">
                    <input type="text" asp-for="Name" placeholder="Your Name">
                    <button type="submit" class="site-btn">Register</button>
                </div>
            </div>
        </form>
    </div>
</div>
<!-- Contact Form End -->